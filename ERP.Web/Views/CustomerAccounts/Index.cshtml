@model ERP.Web.ViewModels.CustomerAccountVM
@using ERP.Web.Utilites
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";

}

@section PageTitle1{كشف حساب عميل}
@section PageTitle2{بحث}
@section Button1{@null}
@section Button1Link{@null}
@section icon{@null}

<div class="row">
    <div class="col-12">
        <div class="card din-bold">
            <div class="card-header">
                <div class="row">
                    <div class="col-sm-6">
                        <h4 class=" din-bold">  كشف حساب</h4>
                    </div>
                </div>

            </div>
            @using (Html.BeginForm("Index", "CustomerAccounts", FormMethod.Get, new { id = "form1" }))
            {
                <div class="card-body din-bold">
                    <div class="row">

                        <div class="col-lg-3">
                            <div class="form-group">
                                <label>العميل</label>
                                @Html.DropDownList("CustomerId", null, "اختر عنصر من القائمة", htmlAttributes: new { @class = "form-select   form-control-solid form-control-lg din-med select2", @onchange = "getBalance();" })
                            </div>
                        </div>
                        <script>
                            function getBalance() {
                                $.get("/SharedDataSources/GetPersonBalance", { id: $("#CustomerId").val(), isCustomer: true }, function (data) {
                                    $("#PesonBalance").val(data);
                                });
                            }
                        </script>
                        <div class="col-lg-3">
                            <div class="form-group">
                                <div>
                                    <label> مديونية العميل</label>
                                    @Html.TextBox("PesonBalance", "0", htmlAttributes: new { @readonly = "readonly", @class = "form-control din-med" })
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3">
                            <div class="form-group">
                                <div>
                                    <label>  الفواتير المعتمدة فقط</label>
                                    @Html.CheckBox("IsFinalApproval", false, htmlAttributes: new { @class = "form-check-input din-med", @style = "width: 25px; height: 25px;" })
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3">
                            <div class="form-group">
                                <div>
                                    <label> الكل بدون نطاق زمنى</label>
                                    @Html.CheckBoxFor(m => m.AllTimes, htmlAttributes: new {  @class = "form-check-input din-med", @style = "width: 25px; height: 25px;" })
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3">
                            <div class="form-group">
                                <div>
                                    <label> من تاريخ</label>
                                    @Html.TextBoxFor(m => m.DtFrom, "{0:yyyy-MM-dd}", htmlAttributes: new { @class = "form-control din-med", @type = "date" })
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3">
                            <div class="form-group">
                                <div>
                                    <label> إلي تاريخ</label>
                                    @Html.TextBoxFor(m => m.DtTo, "{0:yyyy-MM-dd}", htmlAttributes: new { @class = "form-control din-med", @type = "date" })
                                </div>
                            </div>
                        </div>
                        <div class="col-lg-3">
                            <div class="form-group mb-0 w-100">

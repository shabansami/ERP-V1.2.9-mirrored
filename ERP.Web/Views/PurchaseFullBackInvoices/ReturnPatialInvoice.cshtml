                                            <label>طريقة السداد</label>
                                            @Html.DropDownList("PaymentTypeId", null, "اختر عنصر من القائمة", htmlAttributes: new { @class = "form-control din-med", @onchange = "PurchaseFullBackInvoice_Module.onPaymentTypeChanged();" })
                                        </div>
                                        <!--end::Select-->
                                    </div>
                                    <div class="col-xl-4">
                                        <!--begin::Select-->
                                        <div class="form-group">
                                            <label>حساب الدفع</label>
                                            <div class="radio-inline">

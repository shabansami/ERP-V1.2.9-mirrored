                                            @Html.TextBox("ItemName", "", htmlAttributes: new { @class = "form-control din-med w-100", @readonly = "readonly" })
                                            @Html.Hidden("ItemIdd")
                                            @Html.Hidden("QuantityOld", 0)
                                            @Html.Hidden("purchaseDetailsId")
